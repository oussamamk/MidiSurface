<Application x:Class="MidiSurface.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:conv="clr-namespace:MidiSurface.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <conv:BoolToBrushConverter x:Key="BoolToBrushConverter"/>
        <conv:HalfConverter x:Key="HalfConverter"/>
        <conv:KnobPointerConverter x:Key="KnobPointerConverter"/>
    </Application.Resources>
</Application>
